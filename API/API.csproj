    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>f2dbfd69-8422-4e8a-a6a4-36d8c4de0990</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
